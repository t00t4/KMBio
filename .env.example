# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_API_BASE_URL=https://your-api-url.com

# BLE Configuration
EXPO_PUBLIC_BLE_SCAN_TIMEOUT=10000
EXPO_PUBLIC_BLE_CONNECTION_TIMEOUT=5000

# OBD Configuration
EXPO_PUBLIC_OBD_DATA_COLLECTION_FREQUENCY=1000
EXPO_PUBLIC_OBD_RECONNECT_ATTEMPTS=3

# Analytics (Optional)
EXPO_PUBLIC_ANALYTICS_ENABLED=false
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_if_using

# Development
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_MOCK_BLE_DATA=false